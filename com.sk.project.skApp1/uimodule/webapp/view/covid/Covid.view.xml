 <mvc:View controllerName="com.sk.project.skApp1.controller.covid.Covid"
  displayBlock="true"
  xmlns="sap.m"
  xmlns:l="sap.ui.layout"
  xmlns:grid="sap.ui.layout.cssgrid"
  xmlns:core="sap.ui.core"
  xmlns:mvc="sap.ui.core.mvc">
  <Page id="covid" showHeader="false">
   <Panel id='settingsPanel' class="panelStyle" expandable="true" expanded="true" headerText="Settings" width="auto">
   <content>
				<Button type="Accept"
						text="Latest Count by List"
						press="onPressList"
						ariaDescribedBy="acceptButtonDescription genericButtonDescription">
				</Button>
				<Button type="Accept"
						text="Latest Count by Graph"
						press="onPressGraph"
						ariaDescribedBy="rejectButtonDescription genericButtonDescription">
				</Button>
   </content>
   </Panel>
  </Page>
</mvc:View>